{
  "author": "Eirik Albrigtsen <analsandblaster@gmail.com>",
  "name": "logule",
  "description": "A multi-transport, peer-dependent logging library",
  "version": "2.0.0",
  "stability": "stable",
  "repository": {
    "type": "git",
    "url": "clux/logule"
  },
  "keywords": [
    "stdout",
    "emitter",
    "json",
    "file",
    "branch",
    "filtration",
    "configurable"
  ],
  "main": "index.js",
  "scripts": {
    "test": "$(npm bin)/nodeunit --reporter=verbose test/*.js",
    "instrument": "$(npm bin)/jscoverage lib",
    "coverage": "LOGULE_COV=1 $(npm bin)/nodeunit --reporter=lcov test/*.js",
    "coveralls": "npm run coverage | coveralls"
  },
  "dependencies": {
    "dye": "~0.1.0",
    "autonomy": "~0.5",
    "subset": "~0.1.3",
    "confortable": "~0.2.0",
    "typr": "~0.2.0"
  },
  "devDependencies": {
    "nodeunit": "^0.9.0",
    "jscoverage": "^0.5.5",
    "coveralls": "^2.11.1"
  },
  "bugs": {
    "url": "http://github.com/clux/logule/issues"
  },
  "license": "MIT"
}
